<Window x:Class="WPF_Regex.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Regex"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label Content="Telefonszám" />
            <TextBox x:Name="tbxTelefonszám"/>
            <Button Content="Ellenőrzés" Name="btnEllenoriz" Click="btnEllenoriz_Click"  Margin="0 10 0 40"/>
            <Label Content="Ipv4 Cím"/>
            <TextBox x:Name="tbxIpCim"/>



        </StackPanel>
       

    </Grid>
</Window>
